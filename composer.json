{
    "name": "websea/laravel-visitors-statistics",
    "description": "Simple visitor tracker and statistics package for Laravel 5 that can be used for dashboard graphs.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksa Nikolic",
            "email": "aleksa@codelab.rs"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1.3",
        "ext-zlib": "*",
        "illuminate/http": "^7",
        "illuminate/database": "^7",
        "illuminate/routing": "^7",
        "illuminate/support": "^7",
        "piwik/device-detector": "^3.11",
        "maxmind-db/reader": "^1.4"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "Websea\\LaravelVisitorsStatistics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Websea\\LaravelVisitorsStatistics\\": "src/",
            "Websea\\LaravelVisitorsStatistics\\Tests\\": "tests/"
        }
    }
}
